{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "lineWidth": 80,
  "newLineKind": "lf",
  "useTabs": false,
  "indentWidth": 2,
  "typescript": {
    "quoteStyle": "preferSingle",
    "lineWidth": 80,
    "semiColons": "prefer",
    "quoteProps": "asNeeded"
  },
  "json": {},
  "markdown": {},
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/cdk.out"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.88.1.wasm",
    "https://plugins.dprint.dev/json-0.17.4.wasm",
    "https://plugins.dprint.dev/markdown-0.16.1.wasm"
  ]
}
